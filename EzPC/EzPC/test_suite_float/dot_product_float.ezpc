uint32 dim = 5u ;

def void main () {
  input(SERVER, w, float_fl[dim]) ;
  input(SERVER, b, float_fl) ;
  input(CLIENT, x, float_fl[dim]) ;

  uint32_pl lower = 0u ;
  uint32_pl upper = dim ;

  float_fl acc = 0.0 ;
  for i=[lower - lower : upper] {
    acc = acc + (w[i] * x[i]) ;
  } ;

  output(CLIENT, acc > b ? true : false) ;
}
